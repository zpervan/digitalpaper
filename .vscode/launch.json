{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Backend | Debug",
            "type": "go",
            "preLaunchTask": "Build | Debug | Backend",
            "request": "launch",
            "program": "${cwd}/backend",
            "showLog": true,
        },
        {
            "name": "Attach to process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": "${command:pickGoProcess}",
        },
    ]
}